{
  "name": "react-webpack-template",
  "version": "1.0.0",
  "description": "Template repository for react, webpack, babel application",
  "main": "index.js",
  "scripts": {
    "start:dev": "node_modules/.bin/webpack-dev-server --mode development --config webpack/webpack.base.config.js --open --hot --history-api-fallback",
    "build:prod": "webpack --mode production --config webpack/webpack.prod.config.js --env.NODE_ENV=production --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SteveNield/react-webpack-template.git"
  },
  "author": "Steve Nield - Winter Softworks Limited - steve@wintersoftworks.co.uk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SteveNield/react-webpack-template/issues"
  },
  "homepage": "https://github.com/SteveNield/react-webpack-template#readme",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-jsx-remove-data-test-id": "^2.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.4.2",
    "dotenv-webpack": "^1.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  }
}
